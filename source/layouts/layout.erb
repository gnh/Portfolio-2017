<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Grant Harris |<% if current_page.path == "index.html" %> Designer & Developer <% else %> <%= current_page.data.title %><% end %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="<%= current_page.data.meta_description || 'We partner with forward-thinking startups and companies to conceptualize, build and launch new digital products.' %>">

    <meta name="og:image" content="/images/share-image.png">
    <meta name="og:title" content="Volume7">
    <meta name="og:description" content="<%= current_page.data.meta_description || 'We partner with forward-thinking startups and companies to conceptualize, build and launch new digital products.' %>">
    <meta name="og:url" content="http://www.volume7.io">

    <meta name="twitter:card" content="summary">
    <meta name="twitter:image" content="/images/share-image.png">
    <meta name="twitter:title" content="Volume7">
    <meta name="twitter:description" content="<%= current_page.data.meta_description || 'We partner with forward-thinking startups and companies to conceptualize, build and launch new digital products.' %>">
    <meta name="twitter:url" content="http://www.volume7.io">

    <%= favicon_tag '/favicon.png' %>
    <%= stylesheet_link_tag "main" %>
  </head>
  <body class="c-page js-page">

    <%= partial 'partials/nav' %>
    <%= partial 'partials/mobile_nav' %>

    <div class="c-page-content">
      <%= yield %>
    </div>

    <%= javascript_include_tag 'vendor' %>
    <%= javascript_include_tag 'main' %>
    <%= yield_content :page_scripts %>

    <%= partial 'partials/ga' %>
  </body>
</html>